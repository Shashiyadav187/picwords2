    "./app/main.ts",
    "./app/app.module.ts",
    "./app/app.routing.ts",
    "./app/pipes/keys.pipe.ts",
    "./app/services/i18n.service.ts",
    "./app/pipes/translate.mixin.ts",
    "./app/pipes/translate.pipe.ts",
    "./app/components/app/app.component.ts",
    "./app/components/select-lang/select-lang.component.ts",
    "../common/interfaces.ts"
  ]
}